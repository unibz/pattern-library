{{&gt; organisms-header }}

&lt;main&gt;
  {{# page-hero }}
    {{&gt; organisms-hero-small }}
  {{/ page-hero }}

  {{# page-intro }}
    &lt;section class=&quot;l-section&quot;&gt;
      &lt;div class=&quot;l-container&quot;&gt;
        &lt;div class=&quot;typography&quot;&gt;
          {{&gt; atoms-text-paragraph }}
        &lt;/div&gt;
      &lt;/div&gt;
    &lt;/section&gt;
  {{/ page-intro }}

  {{# page-lectures }}
    &lt;section class=&quot;l-section&quot;&gt;
      &lt;div class=&quot;l-container&quot;&gt;
        {{# lectures }}
          &lt;div class=&quot;typography&quot;&gt;
            {{&gt; atoms-heading-3 }}
          &lt;/div&gt;
          &lt;div class=&quot;blockLectures&quot;&gt;
            {{# lecture }}
              {{&gt; molecules-lecture }}
            {{/ lecture }}
          &lt;/div&gt;
        {{/ lectures }}
      &lt;/div&gt;
    &lt;/section&gt;
  {{/ page-lectures }}

  {{# page-details }}
    &lt;section class=&quot;l-section&quot;&gt;
      &lt;div class=&quot;l-container&quot;&gt;
        &lt;div class=&quot;typography&quot;&gt;
          {{&gt; atoms-text-paragraph }}
        &lt;/div&gt;
      &lt;/div&gt;
    &lt;/section&gt;
  {{/ page-details }}
&lt;/main&gt;

{{# page-footer }}
  {{&gt; organisms-footer }}
{{/ page-footer }}