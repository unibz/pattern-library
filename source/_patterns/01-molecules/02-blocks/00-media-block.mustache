<div class="mediaBlock">
  <a class="mediaBlock_figure" href="{{ url }}">
    {{> atoms-landscape-4x3 (class: "mediaBlock_thumb") }}
  </a>
	<div class="mediaBlock_body">
    {{> atoms-heading-2 (class: "mediaBlock_title") }}
    {{> atoms-text-paragraph (class: "mediaBlock_excerpt") }}
    {{> atoms-button-text-alternate (class: "mediaBlock_btn") }}
	</div>
</div>