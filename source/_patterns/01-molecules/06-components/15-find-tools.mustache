<div class="find_tools">
  {{# search }}
    {{> atoms-button-toggle (class:"find_toolToggle find_toolToggle-search", icon:"i-search") }}
    {{> atoms-forms-input-search (class:"find_toolSearch", icon:"i-search") }}
  {{/ search }}
  {{# filter }}
    {{> atoms-button-toggle (class:"find_toolToggle", icon:"i-filter") }}
  {{/ filter }}
  {{# calendar }}
    {{> atoms-button-toggle (class:"find_toolToggle", icon:"i-calendar") }}
  {{/ calendar }}
</div>